{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"SearchDoctor\",\n  data() {\n    return {\n      mainTag: null,\n      doctors: null\n    };\n  },\n  async mounted() {\n    this.mainTag = this.$route.params.id;\n    await this.getDoctor(this.mainTag);\n  },\n  methods: {\n    async getDoctor(searchTag) {\n      await axios.post('http://localhost:9090/doctors', {\n        tag: searchTag\n      }).then(response => {\n        this.doctors = response.data;\n      });\n\n      // window.location.reload();\n    }\n  }\n};","map":{"version":3,"mappings":"AAkCA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACA;IACA;IACA;EACA;EACAC;IACA;MACA;QACAC;MACA;QACA;MACA;;MAEA;IACA;EACA;AACA","names":["name","data","mainTag","doctors","methods","tag"],"sourceRoot":"src/views","sources":["SearchDoctor.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-row v-for=\"(row, index) in doctors\" :key=\"index\" no-gutters>\n      <v-col v-for=\"(doctor, index) in row\" :key=\"index\" style=\"max-width: 390px\">\n        <v-container style=\"padding: 15px\">\n          <v-card  class=\"grey lighten-5\" elevation=\"5\"  outlined shaped>\n            <v-card-title>{{doctor.name}}</v-card-title>\n            <v-card-subtitle>{{doctor.speciality.join(', ')}}</v-card-subtitle>\n            <v-divider class=\"mx-4\"></v-divider>\n            <v-card-text>{{doctor.address}} <br> {{doctor.location}} </v-card-text>\n\n            <v-card-subtitle>{{doctor.schedule.join(', ')}}</v-card-subtitle>\n            <v-divider class=\"mx-4\"></v-divider>\n            <v-card-actions class=\"pt-0\">\n              <v-btn\n                  text\n                  color=\"teal accent-4\"\n                  @click=\"reveal = false\"\n              >\n                Learn More\n              </v-btn>\n              <v-btn\n                  text\n                  color=\"teal accent-4\"\n                  @click=\"reveal = false\"\n              >\n                Schedule\n              </v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-container>\n      </v-col></v-row></v-container>\n</template>\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"SearchDoctor\",\n  data () {\n    return {\n      mainTag : null,\n      doctors : null,\n    }\n  },\n  async mounted() {\n    this.mainTag = this.$route.params.id;\n    await this.getDoctor(this.mainTag)\n  },\n  methods : {\n    async getDoctor(searchTag){\n      await axios.post('http://localhost:9090/doctors', {\n        tag : searchTag,\n      }).then(response => {\n        this.doctors = response.data;\n      });\n\n      // window.location.reload();\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module"}